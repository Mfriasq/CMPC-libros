{
  "name": "nestjs-react-postgres-docker",
  "version": "1.0.0",
  "description": "Fullstack project with NestJS, React, PostgreSQL and Docker",
  "main": "index.js",
  "scripts": {
    "dev": "docker-compose -f docker-compose.dev.yml up --build",
    "dev:detached": "docker-compose -f docker-compose.dev.yml up --build -d",
    "prod": "docker-compose up --build",
    "prod:detached": "docker-compose up --build -d",
    "stop": "docker-compose down",
    "stop:dev": "docker-compose -f docker-compose.dev.yml down",
    "clean": "docker-compose down -v && docker system prune -f",
    "logs": "docker-compose logs -f",
    "logs:backend": "docker-compose logs -f backend",
    "logs:frontend": "docker-compose logs -f frontend",
    "logs:db": "docker-compose logs -f database",
    "backend:install": "cd backend && npm install",
    "frontend:install": "cd frontend && npm install",
    "install:all": "npm run backend:install && npm run frontend:install",
    "backend:dev": "cd backend && npm run start:dev",
    "frontend:dev": "cd frontend && npm start",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build"
  },
  "keywords": [
    "nestjs",
    "react",
    "typescript",
    "postgresql",
    "docker",
    "fullstack"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "ansi-regex": "^5.0.1",
    "ansi-styles": "^4.3.0",
    "axios": "^1.12.2",
    "chalk": "^4.1.2",
    "cliui": "^8.0.1",
    "color-convert": "^2.0.1",
    "color-name": "^1.1.4",
    "date-fns": "^2.30.0",
    "emoji-regex": "^8.0.0",
    "escalade": "^3.2.0",
    "form-data": "^4.0.4",
    "get-caller-file": "^2.0.5",
    "has-flag": "^4.0.0",
    "is-fullwidth-code-point": "^3.0.0",
    "lodash": "^4.17.21",
    "require-directory": "^2.1.1",
    "rxjs": "^7.8.2",
    "shell-quote": "^1.8.3",
    "spawn-command": "^0.0.2",
    "string-width": "^4.2.3",
    "strip-ansi": "^6.0.1",
    "supports-color": "^8.1.1",
    "tree-kill": "^1.2.2",
    "tslib": "^2.8.1",
    "wrap-ansi": "^7.0.0",
    "y18n": "^5.0.8",
    "yargs": "^17.7.2",
    "yargs-parser": "^21.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mfriasq/CMPC-libros.git"
  },
  "bugs": {
    "url": "https://github.com/Mfriasq/CMPC-libros/issues"
  },
  "homepage": "https://github.com/Mfriasq/CMPC-libros#readme"
}
